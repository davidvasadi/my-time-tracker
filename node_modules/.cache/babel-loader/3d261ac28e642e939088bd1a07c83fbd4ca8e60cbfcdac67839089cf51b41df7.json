 return false;\n        };\n        filtered[userName] = userEntries.filter(entry => {\n          const matchesTagFilter = this.tagFilters.length === 0 || entry.tags.some(tag => this.tagFilters.includes(tag));\n          return viewFilter(entry) && matchesTagFilter;\n        });\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    handleFormSubmit(entry) {\n      if (!this.entries[entry.userName]) {\n        this.$set(this.entries, entry.userName, []);\n      }\n      this.entries[entry.userName].push(entry);\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n    },\n    editEntry(entry, userName) {\n      const index = this.entries[userName].indexOf(entry);\n      this.$refs.entryForm.setFormData(entry);\n      this.deleteEntry(userName, index);\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n    toggleTagFilter(tag) {\n      const index = this.tagFilters.indexOf(tag);\n      if (index === -1) {\n        this.tagFilters.push(tag);\n      } else {\n        this.tagFilters.splice(index, 1);\n      }\n    },\n    clearTagFilters() {\n      this.tagFilters = [];\n    }\n  }\n};","map":{"version":3,"names":["TimeEntryForm","components","data","entries","JSON","parse","localStorage","getItem","viewType","tagFilters","computed","uniqueTags","tags","Set","userEntries","Object","values","entry","forEach","tag","add","Array","from","filteredEntries","filtered","todayString","Date","toISOString","split","userName","viewFilter","date","weekStart","setDate","getDate","getDay","startsWith","filter","matchesTagFilter","length","some","includes","methods","handleFormSubmit","$set","push","setItem","stringify","$refs","entryForm","resetForm","editEntry","index","indexOf","setFormData","delete