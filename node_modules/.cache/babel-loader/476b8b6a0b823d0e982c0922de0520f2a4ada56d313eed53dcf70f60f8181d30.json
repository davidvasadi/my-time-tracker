ySubmit(newEntry) {\n      if (this.isEdit) {\n        this.updateEntry(this.currentUserName, this.currentEntry.index, newEntry);\n        this.isEdit = false; // Reset the edit state\n        this.currentEntry = {}; // Reset the current entry\n      } else {\n        this.addEntry(newEntry);\n      }\n    },\n    addEntry(newEntry) {\n      const { userName } = newEntry;\n      if (!this.entries[userName]) {\n        this.entries[userName] = [];\n      }\n      this.entries[userName].push(newEntry);\n      this.saveEntries();\n    },\n    editEntry(userName, entry, index) {\n      this.currentUserName = userName;\n      this.currentEntry = { ...entry, index }; // Retain the index for updating\n      this.isEdit = true; // Set edit state to true\n      this.$refs.entryForm.setFormData(entry); // Populate form with selected entry\n    },\n    updateEntry(userName, index, updatedEntry) {\n      this.entries[userName][index] = updatedEntry; // Update the entry\n      this.saveEntries();\n    },\n    deleteEntry(userName, index) {\n      if (this.entries[userName]) {\n        this.entries[userName].splice(index, 1);\n        if (this.entries[userName].length === 0) {\n          delete this.entries[userName]; // Delete the user if no entries remain\n        }\n        this.saveEntries();\n      }\n    },\n    saveEntries() {\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Custom styles for your App.vue */\n</style>\n"],"mappings":";AAyBA,OAAOA,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,KAAK,CAAC,CAAC;MAC1DC,YAAY,EAAE,CAAC,CAAC;MAChBC,eAAe,EAAE,EAAE;MACnBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAiBA,CAACC,QAAQ,EAAE;MAC1B,IAAI,IAAI,CAACH,MAAM,EAAE;QACf,IAAI,CAACI,WAAW,CAAC,IAAI,CAACL,eAAe,EAAE,IAAI,CAACD,YAAY,CAACO,KAAK,EAAEF,QAAQ,CAAC;QACzE,IAAI,CAACH,MAAK,GAAI,KAAK,E