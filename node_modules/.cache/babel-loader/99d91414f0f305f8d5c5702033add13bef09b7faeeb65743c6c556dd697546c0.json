{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport NotificationMessage from './components/NotificationMessage.vue';\nexport default {\n  components: {\n    TimeEntryForm,\n    NotificationMessage\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      tagFilter: '',\n      notificationMessage: '',\n      isEdit: false,\n      currentEntry: null\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(startOfWeek);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch = this.viewType === 'daily' && entry.date === todayString || this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek || this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth() && entryDate.getFullYear() === today.getFullYear();\n          const isTagMatch = !this.tagFilter || entry.tags.includes(this.tagFilter);\n          return isDateMatch && isTagMatch;\n        });\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      const userName = formData.userName;\n      if (!this.entries[userName]) {\n        this.entries[userName] = [];\n      }\n      if (this.isEdit) {\n        const entryIndex = this.entries[userName].findIndex(entry => entry.date === formData.date && entry.startTime === formData.startTime);\n        if (entryIndex !== -1) {\n          this.entries[userName][entryIndex] = formData;\n        }\n      } else {\n        this.entries[userName].push(formData);\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n      this.notificationMessage = this.isEdit ? 'Bejegyzés sikeresen módosítva!' : 'Bejegyzés sikeresen mentve!';\n      this.isEdit = false;\n    },\n    editEntry(entry) {\n      this.currentEntry = {\n        ...entry\n      };\n      this.isEdit = true;\n      this.scrollToTop();\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.notificationMessage = 'Bejegyzés törölve!';\n    },\n    scrollToTop() {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n    }\n  }\n};","map":{"version":3,"names":["TimeEntryForm","NotificationMessage","components","data","entries","JSON","parse","localStorage","getItem","viewType","tagFilter","notificationMessage","isEdit","currentEntry","computed","filteredEntries","filtered","today","Date","todayString","toISOString","split","startOfWeek","setDate","getDate","getDay","endOfWeek","userName","userEntries","filter","entry","entryDate","date","isDateMatch","getMonth","getFullYear","isTagMatch","tags","includes","methods","handleFormSubmit","formData","entryIndex","findIndex","startTime","push","setItem","stringify","$refs","entryForm","resetForm","editEntry","scrollToTop","deleteEntry","index","splice","length","window","scrollTo","top","behavior"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-4xl mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-6\">Időnyilvántartó rendszer</h1>\n    <TimeEntryForm \n      @submit=\"handleFormSubmit\" \n      ref=\"entryForm\" \n      :entry=\"currentEntry\" \n      :isEdit=\"isEdit\" \n    />\n\n    <!-- Nézet kiválasztása -->\n    <div class=\"mt-6 mb-4\">\n      <label for=\"viewType\" class=\"mr-2\">Nézet:</label>\n      <select v-model=\"viewType\" id=\"viewType\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"daily\">Napi</option>\n        <option value=\"weekly\">Heti</option>\n        <option value=\"monthly\">Havi</option>\n      </select>\n    </div>\n\n    <!-- Szűrés címkére -->\n    <div class=\"mb-4\">\n      <label for=\"tagFilter\" class=\"mr-2\">Szűrés címkére:</label>\n      <select v-model=\"tagFilter\" id=\"tagFilter\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"\">Összes</option>\n        <option value=\"projekt\">Projekt</option>\n        <option value=\"ügyfél\">Ügyfél</option>\n      </select>\n    </div>\n\n    <!-- Bejegyzések -->\n    <div class=\"mt-6\">\n      <h2 class=\"text-xl font-semibold\">Bejegyzések</h2>\n      <div v-if=\"Object.keys(filteredEntries).length === 0\" class=\"mt-4\">\n        <p>Nincsenek bejegyzések.</p>\n      </div>\n      <div v-for=\"(userEntries, userName) in filteredEntries\" :key=\"userName\" class=\"mb-4\">\n        <div v-for=\"(entry, index) in userEntries\" :key=\"index\" class=\"border p-4 mt-2 rounded\">\n          <p><strong>Munkavállaló neve:</strong> {{ entry.userName }}</p>\n          <p><strong>Dátum:</strong> {{ entry.date }}</p>\n          <p><strong>Kezdési idő:</strong> {{ entry.startTime }}</p>\n          <p><strong>Befejezési idő:</strong> {{ entry.endTime }}</p>\n          <p><strong>Feladat leírása:</strong> {{ entry.taskDescription }}</p>\n          <p><strong>Címkék:</strong> <span v-if=\"entry.tags.length\">{{ entry.tags }}</span><span v-else>Nincsenek címkék.</span></p>\n          <button @click=\"editEntry(entry)\" class=\"mt-2 bg-yellow-500 text-white p-2 rounded\">Módosítás</button>\n          <button @click=\"deleteEntry(userName, index)\" class=\"mt-2 bg-red-500 text-white p-2 rounded\">Törlés</button>\n        </div>\n      </div>\n    </div>\n\n    <!-- Értesítések -->\n    <NotificationMessage \n      v-if=\"notificationMessage\" \n      :message=\"notificationMessage\" \n      type=\"success\" \n      @close=\"notificationMessage = ''\" \n    />\n  </div>\n</template>\n\n<script>\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport NotificationMessage from './components/NotificationMessage.vue';\n\nexport default {\n  components: {\n    TimeEntryForm,\n    NotificationMessage,\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      tagFilter: '',\n      notificationMessage: '',\n      isEdit: false,\n      currentEntry: null,\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(startOfWeek);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch =\n            (this.viewType === 'daily' && entry.date === todayString) ||\n            (this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek) ||\n            (this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth() && entryDate.getFullYear() === today.getFullYear());\n          const isTagMatch = !this.tagFilter || entry.tags.includes(this.tagFilter);\n\n          return isDateMatch && isTagMatch;\n        });\n      }\n\n      return filtered;\n    },\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      const userName = formData.userName;\n      if (!this.entries[userName]) {\n        this.entries[userName] = [];\n      }\n      if (this.isEdit) {\n        const entryIndex = this.entries[userName].findIndex(entry => \n          entry.date === formData.date && \n          entry.startTime === formData.startTime\n        );\n        if (entryIndex !== -1) {\n          this.entries[userName][entryIndex] = formData;\n        }\n      } else {\n        this.entries[userName].push(formData);\n      }\n\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n      this.notificationMessage = this.isEdit ? 'Bejegyzés sikeresen módosítva!' : 'Bejegyzés sikeresen mentve!';\n      this.isEdit = false; \n    },\n    editEntry(entry) {\n      this.currentEntry = { ...entry };\n      this.isEdit = true; \n      this.scrollToTop(); \n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.notificationMessage = 'Bejegyzés törölve!';\n    },\n    scrollToTop() {\n      window.scrollTo({ top: 0, behavior: 'smooth' });\n    },\n  },\n};\n</script>\n\n<style>\n/* Custom styles for your application */\n</style>\n"],"mappings":";;;AA6DA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,mBAAkB,MAAO,sCAAsC;AAEtE,eAAe;EACbC,UAAU,EAAE;IACVF,aAAa;IACbC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,KAAK,CAAC,CAAC;MAC1DC,QAAQ,EAAE,OAAO;MACjBC,SAAS,EAAE,EAAE;MACbC,mBAAmB,EAAE,EAAE;MACvBC,MAAM,EAAE,KAAK;MACbC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,MAAMC,QAAO,GAAI,CAAC,CAAC;MACnB,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAAC,CAAC;MACxB,MAAMC,WAAU,GAAIF,KAAK,CAACG,WAAW,CAAC,CAAC,CAACC,KAAK,CAA