{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nexport default {\n  props: {\n    entries: {\n      type: Array,\n      default: () => []\n    }\n  },\n  computed: {\n    totalEntries() {\n      return this.entries.length;\n    },\n    totalHours() {\n      return this.entries.reduce((acc, entry) => {\n        const startTime = new Date(`1970-01-01T${entry.startTime}:00`);\n        const endTime = new Date(`1970-01-01T${entry.endTime}:00`);\n        const hours = (endTime - startTime) / 1000 / 60 / 60; // Konvertálás órákra\n        return acc + hours;\n      }, 0).toFixed(2); // Két tizedesjegy\n    },\n    tagCounts() {\n      const counts = {};\n      this.entries.forEach(entry => {\n        entry.tags.forEach(tag => {\n          counts[tag] = (counts[tag] || 0) + 1;\n        });\n      });\n      return counts;\n    }\n  }\n};","map":{"version":3,"names":["props","entries","type","Array","default","computed","totalEntries","length","totalHours","reduce","acc","entry","startTime","Date","endTime","hours","toFixed","tagCounts","counts","forEach","tags","tag"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/components/MyStatistics.vue"],"sourcesContent":["<template>\n  <div class=\"p-4 bg-white rounded shadow\">\n    <h2 class=\"text-lg font-semibold mb-4\">Statisztikák</h2>\n    <div v-if=\"entries.length === 0\" class=\"text-gray-500\">\n      Nincsenek bejegyzések.\n    </div>\n    <div v-else>\n      <div class=\"mb-4\">\n        <p><strong>Összes bejegyzés:</strong> {{ totalEntries }}</p>\n        <p><strong>Összes munkaóra:</strong> {{ totalHours }} óra</p>\n      </div>\n      <div class=\"mb-4\">\n        <h3 class=\"text-md font-semibold\">Címkék szerinti bontás:</h3>\n        <ul>\n          <li v-for=\"(count, tag) in tagCounts\" :key=\"tag\">\n            {{ tag }}: {{ count }} bejegyzés\n          </li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    entries: {\n      type: Array,\n      default: () => [],\n    },\n  },\n  computed: {\n    totalEntries() {\n      return this.entries.length;\n    },\n    totalHours() {\n      return this.entries.reduce((acc, entry) => {\n        const startTime = new Date(`1970-01-01T${entry.startTime}:00`);\n        const endTime = new Date(`1970-01-01T${entry.endTime}:00`);\n        const hours = (endTime - startTime) / 1000 / 60 / 60; // Konvertálás órákra\n        return acc + hours;\n      }, 0).toFixed(2); // Két tizedesjegy\n    },\n    tagCounts() {\n      const counts = {};\n      this.entries.forEach(entry => {\n        entry.tags.forEach(tag => {\n          counts[tag] = (counts[tag] || 0) + 1;\n        });\n      });\n      return counts;\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Stílusok a statisztikákhoz */\n</style>\n"],"mappings":";;;AAwBA,eAAe;EACbA,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,KAAK;MACXC,OAAO,EAAEA,CAAA,KAAM;IACjB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACL,OAAO,CAACM,MAAM;IAC5B,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACP,OAAO,CAACQ,MAAM,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAK;QACzC,MAAMC,SAAQ,GAAI,IAAIC,IAAI,CAAC,cAAcF,KAAK,CAACC,SAAS,KAAK,CAAC;QAC9D,MAAME,OAAM,GAAI,IAAID,IAAI,CAAC,cAAcF,KAAK,CAACG,OAAO,KAAK,CAAC;QAC1D,MAAMC,KAAI,GAAI,CAACD,OAAM,GAAIF,SAAS,IAAI,IAAG,GAAI,EAAC,GAAI,EAAE,EAAE;QACtD,OAAOF,GAAE,GAAIK,KAAK;MACpB,CAAC,EAAE,CAAC,CAAC,CAACC,OAAO,CAAC,CAAC,CAAC,EAAE;IACpB,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,MAAMC,MAAK,GAAI,CAAC,CAAC;MACjB,IAAI,CAACjB,OAAO,CAACkB,OAAO,CAACR,KAAI,IAAK;QAC5BA,KAAK,CAACS,IAAI,CAACD,OAAO,CAACE,GAAE,IAAK;UACxBH,MAAM,CAACG,GAAG,IAAI,CAACH,MAAM,CAACG,GAAG,KAAK,CAAC,IAAI,CAAC;QACtC,CAAC,CAAC;MACJ,CAAC,CAAC;MACF,OAAOH,MAAM;IACf;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}