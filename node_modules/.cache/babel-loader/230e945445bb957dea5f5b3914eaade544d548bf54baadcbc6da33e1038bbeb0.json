{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      newEntry: {\n        userName: '',\n        date: '',\n        startTime: '',\n        endTime: '',\n        taskDescription: ''\n      },\n      entries: {}\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName].filter(entry => entry.userName && entry.date && entry.startTime && entry.endTime && entry.taskDescription);\n        if (userEntries.length > 0) {\n          filtered[userName] = userEntries;\n        }\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    handleFormSubmit() {\n      const {\n        userName,\n        date,\n        startTime,\n        endTime,\n        taskDescription\n      } = this.newEntry;\n      if (!this.entries[userName]) {\n        this.entries[userName] = [];\n      }\n      this.entries[userName].push({\n        userName,\n        date,\n        startTime,\n        endTime,\n        taskDescription\n      });\n      this.resetForm();\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n    },\n    editEntry(userName, index) {\n      const entry = this.entries[userName][index];\n      this.newEntry = {\n        ...entry\n      };\n      this.deleteEntry(userName, index); // Eltávolítjuk a régi bejegyzést\n    },\n    resetForm() {\n      this.newEntry = {\n        userName: '',\n        date: '',\n        startTime: '',\n        endTime: '',\n        taskDescription: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["data","newEntry","userName","date","startTime","endTime","taskDescription","entries","computed","filteredEntri