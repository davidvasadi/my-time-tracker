edEntries() {\n      const filtered = {};\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName].filter(entry =>\n          entry.userName && entry.date && entry.startTime && entry.endTime && entry.taskDescription\n        );\n        if (userEntries.length > 0) {\n          filtered[userName] = userEntries;\n        }\n      }\n      return filtered;\n    },\n  },\n  methods: {\n    handleFormSubmit() {\n      const { userName, date, startTime, endTime, taskDescription } = this.newEntry;\n\n      if (!this.entries[userName]) {\n        this.entries[userName] = [];\n      }\n\n      this.entries[userName].push({ userName, date, startTime, endTime, taskDescription });\n      this.resetForm();\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n    },\n    editEntry(userName, index) {\n      const entry = this.entries[userName][index];\n      this.newEntry = { ...entry };\n      this.deleteEntry(userName, index); // Eltávolítjuk a régi bejegyzést\n    },\n    resetForm() {\n      this.newEntry = {\n        userName: '',\n        date: '',\n        startTime: '',\n        endTime: '',\n        taskDescription: '',\n      };\n    },\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 800px;\n  margin: auto;\n}\n</style>\n"],"mappings":";;;AA4BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,IAAI,EAAE,EAAE;QACRC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,eAAe,EAAE;MACnB,CAAC;MACDC,OAAO,EAAE,CAAC;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,MAAMC,QAAO,GAAI,CAAC,CAAC;MACnB,KAAK,MAAMR,QAAO,IAAK,IAAI,CAACK,OAAO,EAAE;QACnC,MAAMI,WAAU,GAAI,IAAI,CAACJ,OAAO,CAACL,QAAQ,CAAC,CAACU,MAAM,CAACC,KAAI,IACpDA,KAAK,CAACX,QAAO,IAAKW,KAAK,CAACV,IAAG,IAAKU,KAAK,CAACT,SAAQ,IAAKS,KAAK,CAACR,OAAM,IAAKQ,KAAK,CAACP,eAC5E,CAAC;QACD,IAAIK,WAAW,CAACG,MAA