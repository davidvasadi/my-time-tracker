{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nexport default {\n  components: {\n    TimeEntryForm\n  },\n  data() {\n    return {\n      entries: {},\n      editingIndex: null,\n      editingUserName: ''\n    };\n  },\n  methods: {\n    saveEntry(entry) {\n      if (this.editingIndex !== null && this.editingUserName) {\n        // Módosítás\n        this.entries[this.editingUserName][this.editingIndex] = entry;\n      } else {\n        // Új bejegyzés hozzáadása\n        if (!this.entries[entry.userName]) {\n          this.entries[entry.userName] = [];\n        }\n        this.entries[entry.userName].push(entry);\n      }\n      this.updateLocalStorage();\n      this.loadEntries();\n    },\n    editEntry(entry, index, user) {\n      this.editingIndex = index;\n      this.editingUserName = user;\n\n      // Módosításkor előre kitöltjük az űrlapot\n      this.$refs.timeEntryForm.setFormData(entry);\n    },\n    deleteEntry(userName, index) {\n      if (this.entries[userName]) {\n        this.entries[userName].splice(index, 1);\n        if (this.entries[userName].length === 0) {\n          delete this.entries[userName];\n        }\n        this.updateLocalStorage();\n        this.loadEntries();\n      }\n    },\n    updateLocalStorage() {\n      localStorage.setItem('timeEntries', JSON.stringify(this.entries));\n    },\n    loadEntries() {\n      const storedEntries = JSON.parse(localStorage.getItem('timeEntries'));\n      this.entries = storedEntries || {};\n    }\n  },\n  mounted() {\n    this.loadEntries();\n  }\n};","map":{"version":3,"names":["TimeEntryForm","components","data","entries","editingIndex","editingUserName","methods","saveEntry","entry","userName","push","updateLocalStorage","loadEntries","editEntry","index","user","$refs","timeEntryForm","setFormData","deleteEntry","splice","length","localStorage","setItem","JSON","stringify","storedEntries","parse","getItem","mounted"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto p-4\">\n    <h1 class=\"text-2xl font-bold mb-4\">Időnyilvántartó Rendszer</h1>\n    <TimeEntryForm @save-entry=\"saveEntry\" />\n    \n    <div v-if=\"Object.keys(entries).length === 0\" class=\"text-gray-500\">Nincsenek bejegyzések.</div>\n    \n    <div v-for=\"(userEntries, user) in entries\" :key=\"user\">\n      <h2 class=\"text-xl font-semibold mb-2\">{{ user }}</h2>\n      <ul class=\"mb-4\">\n        <li v-for=\"(entry, index) in userEntries\" :key=\"index\" class=\"border p-2 mb-2\">\n          <div>{{ entry.date }} - {{ entry.startTime }} - {{ entry.endTime }} - {{ entry.taskDescription }}</div>\n          <button @click=\"editEntry(entry, index, user)\" class=\"text-blue-500\">Módosítás</button>\n          <button @click=\"deleteEntry(user, index)\" class=\"text-red-500 ml-2\">Törlés</button>\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TimeEntryForm from './components/TimeEntryForm.vue';\n\nexport default {\n  components: {\n    TimeEntryForm\n  },\n  data() {\n    return {\n      entries: {},\n      editingIndex: null,\n      editingUserName: ''\n    };\n  },\n  methods: {\n    saveEntry(entry) {\n      if (this.editingIndex !== null && this.editingUserName) {\n        // Módosítás\n        this.entries[this.editingUserName][this.editingIndex] = entry;\n      } else {\n        // Új bejegyzés hozzáadása\n        if (!this.entries[entry.userName]) {\n          this.entries[entry.userName] = [];\n        }\n        this.entries[entry.userName].push(entry);\n      }\n      \n      this.updateLocalStorage();\n      this.loadEntries();\n    },\n    editEntry(entry, index, user) {\n      this.editingIndex = index;\n      this.editingUserName = user;\n      \n      // Módosításkor előre kitöltjük az űrlapot\n      this.$refs.timeEntryForm.setFormData(entry);\n    },\n    deleteEntry(userName, index) {\n      if (this.entries[userName]) {\n        this.entries[userName].splice(index, 1);\n        if (this.entries[userName].length === 0) {\n          delete this.entries[userName];\n        }\n        this.updateLocalStorage();\n        this.loadEntries();\n      }\n    },\n    updateLocalStorage() {\n      localStorage.setItem('timeEntries', JSON.stringify(this.entries));\n    },\n    loadEntries() {\n      const storedEntries = JSON.parse(localStorage.getItem('timeEntries'));\n      this.entries = storedEntries || {};\n    }\n  },\n  mounted() {\n    this.loadEntries();\n  }\n}\n</script>\n\n<style scoped>\n.container {\n  background-color: white;\n}\n</style>\n"],"mappings":";AAqBA,OAAOA,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,CAAC;MACXC,YAAY,EAAE,IAAI;MAClBC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,KAAK,EAAE;MACf,IAAI,IAAI,CAACJ,YAAW,KAAM,IAAG,IAAK,IAAI,CAACC,eAAe,EAAE;QACtD;QACA,IAAI,CAACF,OAAO,CAAC,IAAI,CAACE,eAAe,CAAC,CAAC,IAAI,CAACD,YAAY,IAAII,KAAK;MAC/D,OAAO;QACL;QACA,IAAI,CAAC,IAAI,CAACL,OAAO,CAACK,KAAK,CAACC,QAAQ,CAAC,EAAE;UACjC,IAAI,CAACN,OAAO,CAACK,KAAK,CAACC,QAAQ,IAAI,EAAE;QACnC;QACA,IAAI,CAACN,OAAO,CAACK,KAAK,CAACC,QAAQ,CAAC,CAACC,IAAI,CAACF,KAAK,CAAC;MAC1C;MAEA,IAAI,CAACG,kBAAkB,CAAC,CAAC;MACzB,IAAI,CAACC,WAAW,CAAC,CAAC;IACpB,CAAC;IACDC,SAASA,CAACL,KAAK,EAAEM,KAAK,EAAEC,IAAI,EAAE;MAC5B,IAAI,CAACX,YAAW,GAAIU,KAAK;MACzB,IAAI,CAACT,eAAc,GAAIU,IAAI;;MAE3B;MACA,IAAI,CAACC,KAAK,CAACC,aAAa,CAACC,WAAW,CAACV,KAAK,CAAC;IAC7C,CAAC;IACDW,WAAWA,CAACV,QAAQ,EAAEK,KAAK,EAAE;MAC3B,IAAI,IAAI,CAACX,OAAO,CAACM,QAAQ,CAAC,EAAE;QAC1B,IAAI,CAACN,OAAO,CAACM,QAAQ,CAAC,CAACW,MAAM,CAACN,KAAK,EAAE,CAAC,CAAC;QACvC,IAAI,IAAI,CAACX,OAAO,CAACM,QAAQ,CAAC,CAACY,MAAK,KAAM,CAAC,EAAE;UACvC,OAAO,IAAI,CAAClB,OAAO,CAACM,QAAQ,CAAC;QAC/B;QACA,IAAI,CAACE,kBAAkB,CAAC,CAAC;QACzB,IAAI,CAACC,WAAW,CAAC,CAAC;MACpB;IACF,CAAC;IACDD,kBAAkBA,CAAA,EAAG;MACnBW,YAAY,CAACC,OAAO,CAAC,aAAa,EAAE