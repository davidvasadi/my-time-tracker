ton type=\"submit\" class=\"mt-2 bg-blue-500 text-white p-2 rounded\">\n        {{ isEdit ? 'Módosítás' : 'Hozzáadás' }}\n      </button>\n    </form>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    entry: {\n      type: Object,\n      default: () => ({\n        userName: '',\n        date: '',\n        startTime: '',\n        endTime: '',\n        taskDescription: '',\n        tags: '',\n      }),\n    },\n    isEdit: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  data() {\n    return {\n      formData: {\n        userName: this.entry.userName || '',\n        date: this.entry.date || '',\n        startTime: this.entry.startTime || '',\n        endTime: this.entry.endTime || '',\n        taskDescription: this.entry.taskDescription || '',\n        tags: this.entry.tags || '',\n      },\n    };\n  },\n  watch: {\n    entry: {\n      handler(newEntry) {\n        this.formData = { ...newEntry };\n      },\n      deep: true,\n      immediate: true,\n    },\n    isEdit(newVal) {\n      if (!newVal) {\n        this.resetForm();\n      }\n    },\n  },\n  methods: {\n    handleSubmit() {\n      this.$emit('submit', { ...this.formData });\n      this.resetForm(); \n      this.$emit('notify', 'Sikeresen mentve!'); \n    },\n    resetForm() {\n      this.formData = {\n        userName: '',\n        date: '',\n        startTime: '',\n        endTime: '',\n        taskDescription: '',\n        tags: '',\n      };\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Custom styles for your component */\n</style>\n"],"mappings":"AA6CA,eAAe;EACbA,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO;QACdC,QAAQ,EAAE,EAAE;QACZC,IAAI,EAAE,EAAE;QACRC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,eAAe,EAAE,EAAE;QACnBC,IAAI,EAAE;MACR,CAAC;IACH,CAAC;IACDC,MAAM,EAAE;MACNT,IAAI,EAAEU,OAAO;MACbR,OAAO,EAAE;IACX;EACF,CAAC;EACDS,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRT,QAAQ,EAAE,IAAI,CAACJ,KAAK,CAACI,QAAO,IAAK,EAAE;QACnCC,IAAI,EAAE,IAAI