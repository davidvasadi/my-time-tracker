      filtered[userName] = userEntries.filter(entry => {\n          const matchesTagFilter = this.tagFilters.length === 0 || entry.tags.some(tag => this.tagFilters.includes(tag));\n          return viewFilter(entry) && matchesTagFilter;\n        });\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    handleFormSubmit(entry) {\n      if (!this.entries[entry.userName]) {\n        this.$set(this.entries, entry.userName, []);\n      }\n      this.entries[entry.userName].push(entry);\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n    },\n    editEntry(entry, userName) {\n      const index = this.entries[userName].indexOf(entry);\n      this.$refs.entryForm.setFormData(entry);\n      this.deleteEntry(userName, index);\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n    toggleTagFilter(tag) {\n      const index = this.tagFilters.indexOf(tag);\n      if (index === -1) {\n        this.tagFilters.push(tag);\n      } else {\n        this.tagFilters.splice(index, 1);\n      }\n    },\n    clearTagFilters() {\n      this.tagFilters = [];\n    }\n  }\n};","map":{"version":3,"names":["TimeEntryForm","components","data","entries","JSON","parse","localStorage","getItem","viewType","tagFilters","computed","uniqueTags","tags","Set","userEntries","Object","values","entry","forEach","tag","add","Array","from","filteredEntries","filtered","userName","today","Date","todayString","toISOString","split","startOfWeek","setDate","getDate","getDay","startOfMonth","getFullYear","getMonth","viewFilter","date","filter","matchesTagFilter","length","some","includes","methods","handleFormSubmit","$set","push","setItem","stringify","$refs","entryForm","resetForm","editEntry","index","indexOf","setFormData","deleteEntry","splice","toggleTagFilter","clearTagFilters"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-4xl mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-6\">Időnyilvántartó rendszer</h1>\n    <TimeEntryForm @submit=\"handleFormSubmit\" ref=\"entryForm\" />\n\n    <!-- Címke szűrő -->\n    <div class=\"mt-6 mb-4\">\n      <span class=\"mr-2\">Címkék szűrése:</span>\n      <button v-for=\"tag in uniqueTags\" :key=\"tag\" \n              @click=\"toggleTagFilter(tag)\" \n              :class=\"{'bg-blue-500 text-white': tagFilters.includes(tag), 'bg-gray-300': !tagFilters.includes(tag)}\"\n              class=\"p-2 rounded mr-2\">\n        {{ tag }}\n      </button>\n      <button @click=\"clearTagFilters\" class=\"p-2 bg-red-500 text-white rounded\">Szűrők törlése</button>\n    </div>\n\n    <!-- Nézet kiválasztása -->\n    <div class=\"mt-6 mb-4\">\n      <label for=\"viewType\" class=\"mr-2\">Nézet:</label>\n      <select v-model=\"viewType\" id=\"viewType\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"daily\">Napi</option>\n        <option value=\"weekly\">Heti</option>\n        <option value=\"monthly\">Havi</option>\n      </select>\n    </div>\n\n    <div class=\"mt-6\">\n      <h2 class=\"text-xl font-semibold\">Bejegyzések</h2>\n      <div v-if=\"Object.keys(filteredEntries).length === 0\" class=\"mt-4\">\n        <p>Nincsenek bejegyzések.</p>\n      </div>\n      <div v-for=\"(userEntries, userName) in filteredEntries\" :key=\"userName\" class=\"mb-4\">\n        <div v-for=\"(entry, index) in userEntries\" :key=\"index\" class=\"border p-4 mt-2 rounded\">\n          <p><strong>Munkavállaló neve:</strong> {{ entry.userName }}</p>\n          <p><strong>Dátum:</strong> {{ entry.date }}</p>\n          <p><strong>Kezdési idő:</strong> {{ entry.startTime }}</p>\n          <p><strong>Befejezési idő:</strong> {{ entry.endTime }}</p>\n          <p><strong>Feladat leírása:</strong> {{ entry.taskDescription }}</p>\n          <p><strong>Címkék:</strong> <span v-if=\"entry.tags.length\">{{ entry.tags.join(', ') }}</span><span v-else>Nincsenek címkék.</span></p>\n          <button @click=\"editEntry(entry, userName)\" class=\"mt-2 bg-yellow-500 text-white p-2 rounded\">Módosítás</button>\n          <button @click=\"deleteEntry(userName, index)\" class=\"mt-2 bg-red-500 text-white p-2 rounded\">Törlés</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TimeEntryForm from './components/TimeEntryForm.vue';\n\nexport default {\n  components: {\n    TimeEntryForm,\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      tagFilters: [],\n    };\n  },\n  computed: {\n    uniqueTags() {\n      const tags = new Set();\n      for (const userEntries of Object.values(this.entries)) {\n        for (const entry of userEntries) {\n          entry.tags.forEach(tag => tags.add(tag));\n        }\n      }\n      return Array.from(tags);\n    },\n    filteredEntries() {\n      const filtered = {};\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n\n        const today = new Date();\n        const todayString = today.toISOString().split('T')[0];\n        const startOfWeek = new Date(today);\n        startOfWeek.setDate(today.getDate() - today.getDay());\n        const startOfMonth = new Date(today.getFullYear(), today.getMonth(), 1);\n\n        const viewFilter = this.viewType === 'daily'\n          ? entry => entry.date === todayString\n          : this.viewType === 'weekly'\n          ? entry => entry.date >= startOfWeek.toISOString().split('T')[0] && entry.date <= todayString\n          : entry => entry.date >= startOfMonth.toISOString().split('T')[0] && entry.date <= todayString;\n\n        filtered[userName] = userEntries.filter(entry => {\n          const matchesTagFilter = this.tagFilters.length === 0 || entry.tags.some(tag => this.tagFilters.includes(tag));\n          return viewFilter(entry) && matchesTagFilter;\n        });\n      }\n      return filtered;\n    },\n  },\n  methods: {\n    handleFormSubmit(entry) {\n      if (!this.entries[entry.userName]) {\n        this.$set(this.entries, entry.userName, []);\n      }\n      this.entries[entry.userName].push(entry);\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n    },\n    editEntry(entry, userName) {\n      const index = this.entries[userName].indexOf(entry);\n      this.$refs.entryForm.setFormData(entry);\n      this.deleteEntry(userName, index);\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n    toggleTagFilter(tag) {\n      const index = this.tagFilters.indexOf(tag);\n      if (index === -1) {\n        this.tagFilters.push(tag);\n      } else {\n        this.tagFilters.splice(index, 1);\n      }\n    },\n    clearTagFilters() {\n      this.tagFilters = [];\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Custom styles for your component */\n</style>\n"],"mappings":";;;;;;;;;;;;AAiDA,OAAOA,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,KAAK,CAAC,CAAC;MAC1DC,QAAQ,EAAE,OAAO;MACjBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,MAAMC,IAAG,GAAI,IAAIC,GAAG,CAAC,CAAC;MACtB,KAAK,MAAMC,WAAU,IAAKC,MAAM,CAACC,MAAM,CAAC,IAAI,CAACb,OAAO,CAAC,EAAE;QACrD,KAAK,MAAMc,KAAI,IAAKH,WAAW,EAAE;UAC/BG,KAAK,CAACL,IAAI,CAACM,OAAO,CAACC,GAAE,IAAKP,IAAI,CAACQ,GAAG,CAACD,GAAG,CAAC,CAAC;QAC1C;MACF;MACA,OAAOE,KAAK,CAACC,IAAI,CAACV,IAAI,CAAC;IACzB,CAAC;IACDW,eAAeA,CAAA,EAAG;MA