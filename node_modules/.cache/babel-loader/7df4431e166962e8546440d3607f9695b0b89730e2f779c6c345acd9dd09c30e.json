{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport NotificationMessage from './components/NotificationMessage.vue';\nexport default {\n  components: {\n    TimeEntryForm,\n    NotificationMessage\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      tagFilter: '',\n      notificationMessage: '',\n      isEdit: false,\n      currentEntry: {},\n      currentUserName: '',\n      currentEntryIndex: null\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(startOfWeek);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch = this.viewType === 'daily' && entry.date === todayString || this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek || this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth();\n          const isTagMatch = this.tagFilter ? entry.tags === this.tagFilter : true;\n          return isDateMatch && isTagMatch;\n        });\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      const userName = formData.userName;\n      if (this.isEdit) {\n        // Módosítás esetén\n        this.entries[userName][this.currentEntryIndex] = formData;\n        this.isEdit = false; // Módosítás után állítsuk vissza az értéket\n        this.notificationMessage = 'Bejegyzés sikeresen módosítva!';\n      } else {\n        // Új bejegyzés esetén\n        if (!this.entries[userName]) {\n          this.entries[userName] = [];\n        }\n        this.entries[userName].push(formData);\n        this.notificationMessage = 'Bejegyzés sikeresen mentve!';\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n    },\n    editEntry(entry, userName, index) {\n      this.currentEntry = {\n        ...entry\n      };\n      this.isEdit = true; // \n      this.currentUserName = userName; // Az aktuális felhasználó neve\n      this.currentEntryIndex = index; // Az aktuális bejegyzés indexe\n      this.$refs.entryForm.formData = {\n        ...entry\n      }; // A formData beállítása\n      this.scrollToTop(); // Ugrás a lap tetejére\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName]; // Ha az összes bejegyzés törlésre kerül, távolítsuk el a felhasználót\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.notificationMessage = 'Bejegyzés törölve!';\n    },\n    scrollToTop() {\n      window.scrollTo(0, 0); // Azonnali ugrás a lap tetejére\n    }\n  }\n};","map":{"version":3,"names":["TimeEntryForm","NotificationMessage","components","data","entries","JSON","parse","localStorage","getItem","viewType","tagFilter","notificationMessage","isEdit","currentEntry","currentUserName","currentEntryIndex","computed","filteredEntries","filtered","today","Date","todayString","toISOString","split","startOfWeek","setDate","getDate","getDay","endOfWeek","userName","userEntries","filter","entry","entryDate","date","isDateMatch","getMonth","isTagMatch","tags","methods","handleFormSubmit","formData","push","setItem","stringify","$refs","entryForm","resetForm","editEntry","index","scrollToTop","deleteEntry","splice","length","window","scrollTo"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-4xl mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-6\">Időnyilvántartó rendszer</h1>\n    <TimeEntryForm \n      @submit=\"handleFormSubmit\" \n      ref=\"entryForm\" \n      :isEdit=\"isEdit\" \n      :entry=\"currentEntry\" \n    />\n\n    <!-- Nézet kiválasztása -->\n    <div class=\"mt-6 mb-4\">\n      <label for=\"viewType\" class=\"mr-2\">Nézet:</label>\n      <select v-model=\"viewType\" id=\"viewType\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"daily\">Napi</option>\n        <option value=\"weekly\">Heti</option>\n        <option value=\"monthly\">Havi</option>\n      </select>\n    </div>\n\n    <!-- Szűrés címkére -->\n    <div class=\"mb-4\">\n      <label for=\"tagFilter\" class=\"mr-2\">Szűrés címkére:</label>\n      <select v-model=\"tagFilter\" id=\"tagFilter\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"\">Összes</option>\n        <option value=\"projekt\">Projekt</option>\n        <option value=\"ügyfél\">Ügyfél</option>\n      </select>\n    </div>\n\n    <!-- Bejegyzések -->\n    <div class=\"mt-6\">\n      <h2 class=\"text-xl font-semibold\">Bejegyzések</h2>\n      <div v-if=\"Object.keys(filteredEntries).length === 0\" class=\"mt-4\">\n        <p>Nincsenek bejegyzések.</p>\n      </div>\n      <div v-for=\"(userEntries, userName) in filteredEntries\" :key=\"userName\" class=\"mb-4\">\n        <div v-for=\"(entry, index) in userEntries\" :key=\"index\" class=\"border p-4 mt-2 rounded\">\n          <p><strong>Munkavállaló neve:</strong> {{ entry.userName }}</p>\n          <p><strong>Dátum:</strong> {{ entry.date }}</p>\n          <p><strong>Kezdési idő:</strong> {{ entry.startTime }}</p>\n          <p><strong>Befejezési idő:</strong> {{ entry.endTime }}</p>\n          <p><strong>Feladat leírása:</strong> {{ entry.taskDescription }}</p>\n          <p><strong>Címkék:</strong> <span v-if=\"entry.tags.length\">{{ entry.tags }}</span><span v-else>Nincsenek címkék.</span></p>\n          <button @click=\"editEntry(entry, userName, index)\" class=\"mt-2 bg-yellow-500 text-white p-2 rounded\">Módosítás</button>\n          <button @click=\"deleteEntry(userName, index)\" class=\"mt-2 bg-red-500 text-white p-2 rounded\">Törlés</button>\n        </div>\n      </div>\n    </div>\n\n    <!-- Értesítések -->\n    <NotificationMessage \n      v-if=\"notificationMessage\" \n      :message=\"notificationMessage\" \n      type=\"success\" \n      @close=\"notificationMessage = ''\" \n    />\n  </div>\n</template>\n\n<script>\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport NotificationMessage from './components/NotificationMessage.vue';\n\nexport default {\n  components: {\n    TimeEntryForm,\n    NotificationMessage,\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      tagFilter: '',\n      notificationMessage: '',\n      isEdit: false,\n      currentEntry: {},\n      currentUserName: '',\n      currentEntryIndex: null,\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(startOfWeek);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch =\n            (this.viewType === 'daily' && entry.date === todayString) ||\n            (this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek) ||\n            (this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth());\n          const isTagMatch = this.tagFilter ? entry.tags === this.tagFilter : true;\n\n          return isDateMatch && isTagMatch;\n        });\n      }\n      return filtered;\n    },\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      const userName = formData.userName;\n\n      if (this.isEdit) {\n        // Módosítás esetén\n        this.entries[userName][this.currentEntryIndex] = formData;\n        this.isEdit = false; // Módosítás után állítsuk vissza az értéket\n        this.notificationMessage = 'Bejegyzés sikeresen módosítva!';\n      } else {\n        // Új bejegyzés esetén\n        if (!this.entries[userName]) {\n          this.entries[userName] = [];\n        }\n        this.entries[userName].push(formData);\n        this.notificationMessage = 'Bejegyzés sikeresen mentve!';\n      }\n\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n    },\n    editEntry(entry, userName, index) {\n      this.currentEntry = { ...entry };\n      this.isEdit = true; // \n      this.currentUserName = userName; // Az aktuális felhasználó neve\n      this.currentEntryIndex = index; // Az aktuális bejegyzés indexe\n      this.$refs.entryForm.formData = { ...entry }; // A formData beállítása\n      this.scrollToTop(); // Ugrás a lap tetejére\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName]; // Ha az összes bejegyzés törlésre kerül, távolítsuk el a felhasználót\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.notificationMessage = 'Bejegyzés törölve!';\n    },\n    scrollToTop() {\n      window.scrollTo(0, 0); // Azonnali ugrás a lap tetejére\n    },\n  },\n};\n</script>\n\n<style>\n/* Custom styles for your App */\n</style>\n"],"mapping