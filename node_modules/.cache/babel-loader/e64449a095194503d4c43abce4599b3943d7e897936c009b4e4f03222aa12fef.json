{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nexport default {\n  components: {\n    TimeEntryForm\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || [],\n      viewType: 'daily',\n      tagFilter: '',\n      isEdit: false,\n      currentEntry: null,\n      currentEntryIndex: null\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(today);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n      return this.entries.filter(entry => {\n        const entryDate = new Date(entry.date);\n        const isDateMatch = this.viewType === 'daily' && entry.date === todayString || this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek || this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth() && entryDate.getFullYear() === today.getFullYear();\n        const isTagMatch = !this.tagFilter || entry.tags.includes(this.tagFilter);\n        return isDateMatch && isTagMatch;\n      });\n    }\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      if (this.isEdit) {\n        this.entries[this.currentEntryIndex] = formData;\n      } else {\n        this.entries.push(formData);\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.resetForm();\n    },\n    editEntry(entry, index) {\n      this.currentEntry = {\n        ...entry\n      };\n      this.currentEntryIndex = index;\n      this.isEdit = true;\n      this.scrollToTop();\n    },\n    deleteEntry(index) {\n      this.entries.splice(index, 1);\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n    resetForm() {\n      this.$refs.entryForm.resetForm();\n      this.isEdit = false;\n      this.currentEntry = null;\n      this.currentEntryIndex = null;\n    },\n    scrollToTop() {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n    }\n  }\n};","map":{"version":3,"names":["TimeEntryForm","components","data","entries","JSON","parse","localStorage","getItem","viewType","tagFilter","isEdit","currentEntry","currentEntryIndex","computed","filteredEntries","today","Date","todayString","toISOString","split","startOfWeek","setDate","getDate","getDay","endOfWeek","filter","entry","entryDate","date","isDateMatch","getMonth","getFullYear","isTagMatch","tags","includes","methods","handleFormSubmit","formData","push","setItem","stringify","resetForm","editEntry","index","scrollToTop","deleteEntry","splice","$refs","entryForm","window","scrollTo","top","behavior"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-4xl mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-6\">Időnyilvántartó rendszer</h1>\n    <TimeEntryForm \n      @submit=\"handleFormSubmit\" \n      ref=\"entryForm\" \n      :entry=\"currentEntry\" \n      :isEdit=\"isEdit\" \n    />\n    \n    <div class=\"mt-6 mb-4\">\n      <label for=\"viewType\" class=\"mr-2\">Nézet:</label>\n      <select v-model=\"viewType\" id=\"viewType\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"daily\">Napi</option>\n        <option value=\"weekly\">Heti</option>\n        <option value=\"monthly\">Havi</option>\n      </select>\n    </div>\n\n    <div class=\"mb-4\">\n      <label for=\"tagFilter\" class=\"mr-2\">Szűrés címkére:</label>\n      <select v-model=\"tagFilter\" id=\"tagFilter\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"\">Összes</option>\n        <option value=\"projekt\">Projekt</option>\n        <option value=\"ügyfél\">Ügyfél</option>\n      </select>\n    </div>\n\n    <div class=\"mt-6\">\n      <h2 class=\"text-xl font-semibold\">Bejegyzések</h2>\n      <div v-if=\"filteredEntries.length === 0\" class=\"mt-4\">\n        <p>Nincsenek bejegyzések.</p>\n      </div>\n      <div v-for=\"(entry, index) in filteredEntries\" :key=\"index\" class=\"border p-4 mt-2 rounded\">\n        <p><strong>Munkavállaló neve:</strong> {{ entry.userName }}</p>\n        <p><strong>Dátum:</strong> {{ entry.date }}</p>\n        <p><strong>Kezdési idő:</strong> {{ entry.startTime }}</p>\n        <p><strong>Befejezési idő:</strong> {{ entry.endTime }}</p>\n        <p><strong>Feladat leírása:</strong> {{ entry.taskDescription }}</p>\n        <p><strong>Címkék:</strong> <span v-if=\"entry.tags.length\">{{ entry.tags }}</span><span v-else>Nincsenek címkék.</span></p>\n        <button @click=\"editEntry(entry, index)\" class=\"mt-2 bg-yellow-500 text-white p-2 rounded\">Módosítás</button>\n        <button @click=\"deleteEntry(index)\" class=\"mt-2 bg-red-500 text-white p-2 rounded\">Törlés</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TimeEntryForm from './components/TimeEntryForm.vue';\n\nexport default {\n  components: {\n    TimeEntryForm,\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || [],\n      viewType: 'daily',\n      tagFilter: '',\n      isEdit: false,\n      currentEntry: null,\n      currentEntryIndex: null,\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(today);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n\n      return this.entries.filter(entry => {\n        const entryDate = new Date(entry.date);\n        const isDateMatch =\n          (this.viewType === 'daily' && entry.date === todayString) ||\n          (this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek) ||\n          (this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth() && entryDate.getFullYear() === today.getFullYear());\n\n        const isTagMatch = !this.tagFilter || entry.tags.includes(this.tagFilter);\n        return isDateMatch && isTagMatch;\n      });\n    },\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      if (this.isEdit) {\n        this.entries[this.currentEntryIndex] = formData;\n      } else {\n        this.entries.push(formData);\n      }\n\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.resetForm();\n    },\n    editEntry(entry, index) {\n      this.currentEntry = { ...entry };\n      this.currentEntryIndex = index;\n      this.isEdit = true;\n      this.scrollToTop();\n    },\n    deleteEntry(index) {\n      this.entries.splice(index, 1);\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n    resetForm() {\n      this.$refs.entryForm.resetForm();\n      this.isEdit = false;\n      this.currentEntry = null;\n      this.currentEntryIndex = null;\n    },\n    scrollToTop() {\n      window.scrollTo({ top: 0, behavior: 'smooth' });\n    },\n  },\n};\n</script>\n\n<style>\n/* Custom styles for your application */\n</style>\n"],"mappings":";;;AAgDA,OAAOA,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,KAAK,EAAE;MAC1DC,QAAQ,EAAE,OAAO;MACjBC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE,KAAK;MACbC,YAAY,EAAE,IAAI;MAClBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAAC,CAAC;MACxB,MAAMC,WAAU,GAAIF,KAAK,CAACG,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MACrD,MAAMC,WAAU,GAAI,IAAIJ,IAAI,CAACD,KAAK,CAAC;MACnCK,WAAW,CAACC,OAAO,CAACN,KAAK,CAACO,OAAO,CAAC,IAAIP,KAAK,CAACQ,