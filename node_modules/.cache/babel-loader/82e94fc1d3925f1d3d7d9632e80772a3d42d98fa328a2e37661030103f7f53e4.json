{"ast":null,"code":"import \"core-js/modules/esnext.iterator.map.js\";\nimport { ref, watch } from 'vue';\nexport default {\n  name: 'MyStatistics',\n  props: {\n    entries: {\n      type: Array,\n      default: () => []\n    }\n  },\n  setup(props) {\n    const filteredEntries = ref([]);\n    watch(() => props.entries, newEntries => {\n      // Ellenőrizzük, hogy newEntries tömb-e\n      filteredEntries.value = Array.isArray(newEntries) ? newEntries : [];\n      createChart(); // Frissítsük a diagramot\n    }, {\n      immediate: true\n    });\n    const createChart = () => {\n      if (!Array.isArray(filteredEntries.value)) {\n        console.error('filteredEntries.value is not an array');\n        return;\n      }\n      const ctx = document.getElementById('myChart').getContext('2d');\n      const chartData = {\n        labels: filteredEntries.value.map(entry => entry.label),\n        // Címkék az adatokhoz\n        datasets: [{\n          label: 'My Dataset',\n          data: filteredEntries.value.map(entry => entry.value),\n          // Értékek az adatokhoz\n          backgroundColor: 'rgba(75, 192, 192, 0.2)',\n          borderColor: 'rgba(75, 192, 192, 1)',\n          borderWidth: 1\n        }]\n      };\n      const myChart = new Chart(ctx, {\n        type: 'bar',\n        // Válaszd ki a diagram típusát\n        data: chartData,\n        options: {\n          scales: {\n            y: {\n              beginAtZero: true\n            }\n          }\n        }\n      });\n    };\n    return {\n      filteredEntries\n    };\n  }\n};","map":{"version":3,"names":["ref","watch","name","props","entries","type","Array","default","setup","filteredEntries","newEntries","value","isArray","createChart","immediate","console","error","ctx","document","getElementById","getContext","chartData","labels","map","entry","label","datasets","data","backgroundColor","borderColor","borderWidth","myChart","Chart","options","scales","y","beginAtZero"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/components/MyStatistics.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>My Statistics</h2>\n    <div v-if=\"filteredEntries.length === 0\">\n      <p>No entries available.</p>\n    </div>\n    <ul>\n      <li v-for=\"(entry, index) in filteredEntries\" :key=\"index\">\n        {{ entry }}\n      </li>\n    </ul>\n    <!-- Diagram megjelenítése itt -->\n    <canvas id=\"myChart\"></canvas>\n  </div>\n</template>\n\n<script>\nimport { ref, watch } from 'vue';\n\nexport default {\n  name: 'MyStatistics',\n  props: {\n    entries: {\n      type: Array,\n      default: () => [],\n    },\n  },\n  setup(props) {\n    const filteredEntries = ref([]);\n\n    watch(\n      () => props.entries,\n      (newEntries) => {\n        // Ellenőrizzük, hogy newEntries tömb-e\n        filteredEntries.value = Array.isArray(newEntries) ? newEntries : [];\n        createChart(); // Frissítsük a diagramot\n      },\n      { immediate: true }\n    );\n\n    const createChart = () => {\n      if (!Array.isArray(filteredEntries.value)) {\n        console.error('filteredEntries.value is not an array');\n        return;\n      }\n\n      const ctx = document.getElementById('myChart').getContext('2d');\n      const chartData = {\n        labels: filteredEntries.value.map(entry => entry.label), // Címkék az adatokhoz\n        datasets: [{\n          label: 'My Dataset',\n          data: filteredEntries.value.map(entry => entry.value), // Értékek az adatokhoz\n          backgroundColor: 'rgba(75, 192, 192, 0.2)',\n          borderColor: 'rgba(75, 192, 192, 1)',\n          borderWidth: 1,\n        }],\n      };\n\n      const myChart = new Chart(ctx, {\n        type: 'bar', // Válaszd ki a diagram típusát\n        data: chartData,\n        options: {\n          scales: {\n            y: {\n              beginAtZero: true,\n            },\n          },\n        },\n      });\n    };\n\n    return {\n      filteredEntries,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Stílusok itt */\n</style>\n"],"mappings":";AAiBA,SAASA,GAAG,EAAEC,KAAI,QAAS,KAAK;AAEhC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,KAAK;MACXC,OAAO,EAAEA,CAAA,KAAM;IACjB;EACF,CAAC;EACDC,KAAKA,CAACL,KAAK,EAAE;IACX,MAAMM,eAAc,GAAIT,GAAG,CAAC,EAAE,CAAC;IAE/BC,KAAK,CACH,MAAME,KAAK,CAACC,OAAO,EAClBM,UAAU,IAAK;MACd;MACAD,eAAe,CAACE,KAAI,GAAIL,KAAK,CAACM,OAAO,CAACF,UAAU,IAAIA,UAAS,GAAI,EAAE;MACnEG,WAAW,CAAC,CAAC,EAAE;IACjB,CAAC,EACD;MAAEC,SAAS,EAAE;IAAK,CACpB,CAAC;IAED,MAAMD,WAAU,GAAIA,CAAA,KAAM;MACxB,IAAI,CAACP,KAAK,CAACM,OAAO,CAACH,eAAe,CAACE,KAAK,CAAC,EAAE;QACzCI,OAAO,CAACC,KAAK,CAAC,uCAAuC,CAAC;QACtD;MACF;MAEA,MAAMC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,SAAS,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC;MAC/D,MAAMC,SAAQ,GAAI;QAChBC,MAAM,EAAEb,eAAe,CAACE,KAAK,CAACY,GAAG,CAACC,KAAI,IAAKA,KAAK,CAACC,KAAK,CAAC;QAAE;QACzDC,QAAQ,EAAE,CAAC;UACTD,KAAK,EAAE,YAAY;UACnBE,IAAI,EAAElB,eAAe,CAACE,KAAK,CAACY,GAAG,CAACC,KAAI,IAAKA,KAAK,CAACb,KAAK,CAAC;UAAE;UACvDiB,eAAe,EAAE,yBAAyB;UAC1CC,WAAW,EAAE,uBAAuB;UACpCC,WAAW,EAAE;QACf,CAAC;MACH,CAAC;MAED,MAAMC,OAAM,GAAI,IAAIC,KAAK,CAACf,GAAG,EAAE;QAC7BZ,IAAI,EAAE,KAAK;QAAE;QACbsB,IAAI,EAAEN,SAAS;QACfY,OAAO,EAAE;UACPC,MAAM,EAAE;YACNC,CAAC,EAAE;cACDC,WAAW,EAAE;YACf;UACF;QACF;MACF,CAAC,CAAC;IACJ,CAAC;IAED,OAAO;MACL3B;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}