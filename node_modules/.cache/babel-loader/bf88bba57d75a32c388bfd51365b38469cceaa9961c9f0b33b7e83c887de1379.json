{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport MyStatistics from './components/MyStatistics.vue'; // Az új komponenst importáljuk\n\nexport default {\n  components: {\n    TimeEntryForm,\n    MyStatistics // Az új komponenst itt használjuk\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      // Alapértelmezett nézet\n      tagFilter: '' // Címke szűrés\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch = entry.date === todayString;\n          const isTagMatch = !this.tagFilter || entry.tags.includes(this.tagFilter.trim());\n          return isDateMatch && isTagMatch;\n        });\n        if (filtered[userName].length === 0) delete filtered[userName];\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    handleFormSubmit(entry) {\n      const userName = entry.userName;\n      if (!this.entries[userName]) {\n        this.entries[userName] = [];\n      }\n      const existingIndex = this.entries[userName].findIndex(e => e.date === entry.date && e.startTime === entry.startTime);\n      if (existingIndex !== -1) {\n        this.entries[userName][existingIndex] = entry;\n      } else {\n        this.entries[userName].push(entry);\n      }\n      this.saveEntries();\n      this.resetForm();\n    },\n    editEntry(entry) {\n      this.$refs.entryForm.setFormData(entry);\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      this.saveEntries();\n    },\n    saveEntries() {\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n    resetForm() {\n      this.$refs.entryForm.resetForm();\n    }\n  }\n};","map":{"version":3,"names":["TimeEntryForm","MyStatistics","components","data","entries","JSON","parse","localStorage","getItem","viewType","tagFilter","computed","filteredEntries","filtered","today","Date","todayString","toISOString","split","userName","userEntries","filter","entry","entryDate","date","isDateMatch","isTagMatch","tags","includes","trim","length","methods","handleFormSubmit","existingIndex","findIndex","e","startTime","push","saveEntries","resetForm","editEntry","$refs","entryForm","setFormData","deleteEntry","index","splice","setItem","stringify"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-4xl mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-6\">Időnyilvántartó rendszer</h1>\n    <TimeEntryForm @submit=\"handleFormSubmit\" ref=\"entryForm\" />\n\n    <!-- Statisztikák -->\n    <MyStatistics :entries=\"entries\" /> <!-- Az új komponenst itt használjuk -->\n\n    <!-- Bejegyzések -->\n    <div class=\"mt-6\">\n      <h2 class=\"text-xl font-semibold\">Bejegyzések</h2>\n      <div v-if=\"Object.keys(filteredEntries).length === 0\" class=\"mt-4\">\n        <p>Nincsenek bejegyzések.</p>\n      </div>\n      <div v-for=\"(userEntries, userName) in filteredEntries\" :key=\"userName\" class=\"mb-4\">\n        <div v-for=\"(entry, index) in userEntries\" :key=\"index\" class=\"border p-4 mt-2 rounded\">\n          <p><strong>Munkavállaló neve:</strong> {{ entry.userName }}</p>\n          <p><strong>Dátum:</strong> {{ entry.date }}</p>\n          <p><strong>Kezdési idő:</strong> {{ entry.startTime }}</p>\n          <p><strong>Befejezési idő:</strong> {{ entry.endTime }}</p>\n          <p><strong>Feladat leírása:</strong> {{ entry.taskDescription }}</p>\n          <p><strong>Címkék:</strong> <span v-if=\"entry.tags.length\">{{ entry.tags.join(', ') }}</span><span v-else>Nincsenek címkék.</span></p>\n          <button @click=\"editEntry(entry)\" class=\"mt-2 bg-yellow-500 text-white p-2 rounded\">Módosítás</button>\n          <button @click=\"deleteEntry(userName, index)\" class=\"mt-2 bg-red-500 text-white p-2 rounded\">Törlés</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport MyStatistics from './components/MyStatistics.vue'; // Az új komponenst importáljuk\n\nexport default {\n  components: {\n    TimeEntryForm,\n    MyStatistics, // Az új komponenst itt használjuk\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily', // Alapértelmezett nézet\n      tagFilter: '', // Címke szűrés\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch = entry.date === todayString;\n          const isTagMatch = !this.tagFilter || entry.tags.includes(this.tagFilter.trim());\n          return isDateMatch && isTagMatch;\n        });\n\n        if (filtered[userName].length === 0) delete filtered[userName];\n      }\n      return filtered;\n    },\n  },\n  methods: {\n    handleFormSubmit(entry) {\n      const userName = entry.userName;\n      if (!this.entries[userName]) {\n        this.entries[userName] = [];\n      }\n\n      const existingIndex = this.entries[userName].findIndex(\n        e => e.date === entry.date && e.startTime === entry.startTime\n      );\n      if (existingIndex !== -1) {\n        this.entries[userName][existingIndex] = entry;\n      } else {\n        this.entries[userName].push(entry);\n      }\n      this.saveEntries();\n      this.resetForm();\n    },\n    editEntry(entry) {\n      this.$refs.entryForm.setFormData(entry);\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      this.saveEntries();\n    },\n    saveEntries() {\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n    },\n    resetForm() {\n      this.$refs.entryForm.resetForm();\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Custom styles for your application */\n</style>\n"],"mappings":";;;AA+BA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,YAAW,MAAO,+BAA+B,EAAE;;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVF,aAAa;IACbC,YAAY,CAAE;EAChB,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,KAAK,CAAC,CAAC;MAC1DC,QAAQ,EAAE,OAAO;MAAE;MACnBC,SAAS,EAAE,EAAE,CAAE;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,MAAMC,QAAO,GAAI,CAAC,CAAC;MACnB,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAAC,CAAC;MACxB,MAAMC,WAAU,GAAIF,KAAK,CAACG,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAErD,KAAK,MAAMC,QAAO,IAAK,IAAI,CAACf,OAAO,EAAE;QACnC,MAAMgB,WAAU,GAAI,IAAI,CAAChB,OAAO,CAACe,QAAQ,CAAC;QAE1CN,QAAQ,CAACM,QAAQ,IAAIC,WAAW,CAACC,MAAM,CAACC,KAAI,IAAK;UAC/C,MAAMC,SAAQ,GAAI,IAAIR,IAAI,CAACO,KAAK,CAACE,IAAI,CAAC;UACtC,MAAMC,WAAU,GAAIH,KAAK,CAACE,IAAG,KAAMR,WAAW;UAC9C,MAAMU,UAAS,GAAI,CAAC,IAAI,CAAChB,SAAQ,IAAKY,KAAK,CAACK,IAAI,CAACC,QAAQ,CAAC,IAAI,CAAClB,SAAS,CAACmB,IAAI,CAAC,CAAC,CAAC;UAChF,OAAOJ,WAAU,IAAKC,UAAU;QAClC,CAAC,CAAC;QAEF,IAAIb,QAAQ,CAACM,QAAQ,CAAC,CAACW,MAAK,KAAM,CAAC,EAAE,OAAOjB,QAAQ,CAACM,QAAQ,CAAC;MAChE;MACA,OAAON,QAAQ;IACjB;EACF,CAAC;EACDkB,OAAO,EAAE;IACPC,gBAAgBA,C