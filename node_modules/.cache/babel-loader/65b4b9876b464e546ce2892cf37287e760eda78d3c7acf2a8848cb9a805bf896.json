{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport MyStatistics from './components/MyStatistics.vue'; // Győződj meg róla, hogy ezt importálod\n\nexport default {\n  components: {\n    TimeEntryForm,\n    MyStatistics\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      tagFilter: '',\n      toasts: [],\n      isEdit: false,\n      currentEntry: {},\n      currentUserName: '',\n      currentEntryIndex: null\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(startOfWeek);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch = this.viewType === 'daily' && entry.date === todayString || this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek || this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth();\n          const isTagMatch = this.tagFilter ? entry.tags === this.tagFilter : true;\n          return isDateMatch && isTagMatch;\n        });\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      const userName = formData.userName;\n      if (this.isEdit) {\n        this.entries[userName][this.currentEntryIndex] = formData;\n        this.isEdit = false;\n        this.addToast('Bejegyzés sikeresen módosítva!', 'success');\n      } else {\n        if (!this.entries[userName]) {\n          this.entries[userName] = [];\n        }\n        this.entries[userName].push(formData);\n        this.addToast('Bejegyzés sikeresen mentve!', 'success');\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n    },\n    editEntry(entry, userName, index) {\n      this.currentEntry = {\n        ...entry\n      };\n      this.isEdit = true;\n      this.currentUserName = userName;\n      this.currentEntryIndex = index;\n      this.$refs.entryForm.formData = {\n        ...entry\n      };\n      this.scrollToTop();\n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName];\n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.addToast('Bejegyzés törölve!', 'error');\n    },\n    scrollToTop() {\n      window.scrollTo(0, 0);\n    },\n    addToast(message, type) {\n      this.toasts.push({\n        message,\n        type\n      });\n      setTimeout(() => {\n        this.removeToast(this.toasts.length - 1);\n      }, 3000);\n    },\n    removeToast(index) {\n      this.toasts.splice(index, 1);\n    }\n  }\n};","map":{"version":3,"names":["TimeEntryForm","MyStatistics","components","data","entries","JSON","parse","localStorage","getItem","viewType","tagFilter","toasts","isEdit","currentEntry","currentUserName","currentEntryIndex","computed","filteredEntries","filtered","today","Date","todayString","toISOString","split","startOfWeek","setDate","getDate","getDay","endOfWeek","userName","userEntries","filter","entry","entryDate","date","isDateMatch","getMonth","isTagMatch","tags","methods","handleFormSubmit","formData","addToast","push","setItem","stringify","$refs","entryForm","resetForm","editEntry","index","scrollToTop","deleteEntry","splice","length","window","scrollTo","message","type","setTimeout","removeToast"],"sources":["/Users/vasadidavid/Desktop/motibro/my-time-tracker/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-4xl mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-6\">Időnyilvántartó rendszer</h1>\n    <TimeEntryForm \n      @submit=\"handleFormSubmit\" \n      ref=\"entryForm\" \n      :isEdit=\"isEdit\" \n      :entry=\"currentEntry\" \n    />\n\n    <!-- Nézet kiválasztása -->\n    <div class=\"mt-6 mb-4\">\n      <label for=\"viewType\" class=\"mr-2\">Nézet:</label>\n      <select v-model=\"viewType\" id=\"viewType\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"daily\">Napi</option>\n        <option value=\"weekly\">Heti</option>\n        <option value=\"monthly\">Havi</option>\n      </select>\n    </div>\n\n    <!-- Szűrés címkére -->\n    <div class=\"mb-4\">\n      <label for=\"tagFilter\" class=\"mr-2\">Szűrés címkére:</label>\n      <select v-model=\"tagFilter\" id=\"tagFilter\" class=\"p-2 border border-gray-300 rounded\">\n        <option value=\"\">Összes</option>\n        <option value=\"projekt\">Projekt</option>\n        <option value=\"ügyfél\">Ügyfél</option>\n      </select>\n    </div>\n\n    <!-- Bejegyzések -->\n    <div class=\"mt-6\">\n      <h2 class=\"text-xl font-semibold\">Bejegyzések</h2>\n      <div v-if=\"Object.keys(filteredEntries).length === 0\" class=\"mt-4\">\n        <p>Nincsenek bejegyzések.</p>\n      </div>\n      <div v-for=\"(userEntries, userName) in filteredEntries\" :key=\"userName\" class=\"mb-4\">\n        <div v-for=\"(entry, index) in userEntries\" :key=\"index\" class=\"border p-4 mt-2 rounded\">\n          <p><strong>Munkavállaló neve:</strong> {{ entry.userName }}</p>\n          <p><strong>Dátum:</strong> {{ entry.date }}</p>\n          <p><strong>Kezdési idő:</strong> {{ entry.startTime }}</p>\n          <p><strong>Befejezési idő:</strong> {{ entry.endTime }}</p>\n          <p><strong>Feladat leírása:</strong> {{ entry.taskDescription }}</p>\n          <p><strong>Címkék:</strong> <span v-if=\"entry.tags.length\">{{ entry.tags }}</span><span v-else>Nincsenek címkék.</span></p>\n          <button @click=\"editEntry(entry, userName, index)\" class=\"mt-2 bg-yellow-500 hover:bg-yellow-700 text-white p-2 rounded\">Módosítás</button>\n          <button @click=\"deleteEntry(userName, index)\" class=\"mt-2 mx-2 bg-red-500 hover:bg-red-700 text-white p-2 rounded\">Törlés</button>\n        </div>\n      </div>\n    </div>\n\n    <!-- MyStatistics komponens -->\n    <MyStatistics :entries=\"entries\" />\n    <!-- Értesítések (toast) -->\n    <div class=\"fixed top-0 right-0 p-4\">\n      <div v-for=\"(toast, index) in toasts\" :key=\"index\" :class=\"['toast', toast.type]\">\n        {{ toast.message }}\n        <button @click=\"removeToast(index)\" class=\"ml-2 bg-transparent border-0 cursor-pointer\">X</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TimeEntryForm from './components/TimeEntryForm.vue';\nimport MyStatistics from './components/MyStatistics.vue'; // Győződj meg róla, hogy ezt importálod\n\nexport default {\n  components: {\n    TimeEntryForm,\n    MyStatistics,\n  },\n  data() {\n    return {\n      entries: JSON.parse(localStorage.getItem('entries')) || {},\n      viewType: 'daily',\n      tagFilter: '',\n      toasts: [],\n      isEdit: false,\n      currentEntry: {},\n      currentUserName: '',\n      currentEntryIndex: null,\n    };\n  },\n  computed: {\n    filteredEntries() {\n      const filtered = {};\n      const today = new Date();\n      const todayString = today.toISOString().split('T')[0];\n      const startOfWeek = new Date(today);\n      startOfWeek.setDate(today.getDate() - today.getDay());\n      const endOfWeek = new Date(startOfWeek);\n      endOfWeek.setDate(startOfWeek.getDate() + 6);\n\n      for (const userName in this.entries) {\n        const userEntries = this.entries[userName];\n\n        filtered[userName] = userEntries.filter(entry => {\n          const entryDate = new Date(entry.date);\n          const isDateMatch =\n            (this.viewType === 'daily' && entry.date === todayString) ||\n            (this.viewType === 'weekly' && entryDate >= startOfWeek && entryDate <= endOfWeek) ||\n            (this.viewType === 'monthly' && entryDate.getMonth() === today.getMonth());\n          const isTagMatch = this.tagFilter ? entry.tags === this.tagFilter : true;\n\n          return isDateMatch && isTagMatch;\n        });\n      }\n      return filtered;\n    },\n  },\n  methods: {\n    handleFormSubmit(formData) {\n      const userName = formData.userName;\n\n      if (this.isEdit) {\n        this.entries[userName][this.currentEntryIndex] = formData;\n        this.isEdit = false;\n        this.addToast('Bejegyzés sikeresen módosítva!', 'success');\n      } else {\n        if (!this.entries[userName]) {\n          this.entries[userName] = [];\n        }\n        this.entries[userName].push(formData);\n        this.addToast('Bejegyzés sikeresen mentve!', 'success');\n      }\n\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.$refs.entryForm.resetForm();\n    },\n    editEntry(entry, userName, index) {\n      this.currentEntry = { ...entry };\n      this.isEdit = true; \n      this.currentUserName = userName; \n      this.currentEntryIndex = index; \n      this.$refs.entryForm.formData = { ...entry }; \n      this.scrollToTop(); \n    },\n    deleteEntry(userName, index) {\n      this.entries[userName].splice(index, 1);\n      if (this.entries[userName].length === 0) {\n        delete this.entries[userName]; \n      }\n      localStorage.setItem('entries', JSON.stringify(this.entries));\n      this.addToast('Bejegyzés törölve!', 'error'); \n    },\n    scrollToTop() {\n      window.scrollTo(0, 0); \n    },\n    addToast(message, type) {\n      this.toasts.push({ message, type });\n      setTimeout(() => {\n        this.removeToast(this.toasts.length - 1); \n      }, 3000);\n    },\n    removeToast(index) {\n      this.toasts.splice(index, 1);\n    },\n  },\n};\n</script>\n\n<style>\n.toast {\n  @apply p-2 mb-2 rounded shadow-md;\n}\n.toast.success {\n  @apply bg-green-100 text-green-800;\n}\n.toast.error {\n  @apply bg-red-100 text-red-800;\n}\n</style>\n"],"mappings":";;;AA+DA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,YAAW,MAAO,+BAA+B,EAAE;;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVF,aAAa;IACbC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,KAAK,CAAC,CAAC;MAC1DC,QAAQ,EAAE,OAAO;MACjBC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,KAAK;MACbC,YAAY,EAAE,CAAC,CAAC;MAChBC,eAAe,EAAE,EAAE;MACnBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,MAAMC,QAAO,GAAI,CAAC,CAAC;MACnB,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAAC,CAAC;MACxB,MAAMC,WAAU,GAAIF,KAAK,CAACG,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MACrD,MAAMC,WAAU,GAAI,IAAIJ,IAAI,CAACD,KAAK,CAAC;MACnCK,WAAW,CAACC,OAAO,CAACN,KAAK,CAACO,OAAO,CAAC,IAAIP,KAAK,CAACQ,MAAM,CAAC,CAAC,CAAC;MACrD,MAAMC,SAAQ,GAAI,IAAIR,IAAI,CAACI,WAAW,CAAC;MACvCI,SAAS,CAACH,OAAO,CAACD,WAAW,CAACE,OAAO,CAAC,IAAI,CAAC,CAAC;MAE5C,KAAK,MAAMG,QAAO,IAAK,IAAI,CAACzB,OAAO,EAAE;QACnC,MAAM0B,WAAU,GAAI,IAAI,CAAC1B,OAAO,CAACyB,QAAQ,CAAC;QAE1CX,QAAQ,CAACW,QAAQ,IAAIC,WAAW,CAACC,MAAM,CAACC,KAAI,IAAK;UAC/C,MAAMC,SAAQ,GAAI,IAAIb,IAAI,CAACY,KAAK,CAACE,IAAI,CAAC;UACtC,MAAMC,WAAU,GACb,IAAI,CAAC1B,QAAO,KAAM,OAAM,IAAKuB,KAAK,CAACE,IAAG,KAAMb,WAAW,IACvD,IAAI,CAACZ,QAAO,KAAM,QAAO,IAAKwB,SAAQ,IAAKT,WAAU,IAAKS,SAAQ,IAAKL,SAAS,IAChF,IAAI,CAACnB,QAAO,KAAM,SAAQ,IAAKwB,SAAS,CAACG,QAAQ,CAAC,MAAMjB,KAAK,CAACiB,QAAQ,CAAC,CAAE;UAC5E,MAAMC,UAAS,GAAI,IAAI,CAAC3B,SAAQ,GAAIsB,KAAK,CAACM,IAAG,KAAM,IAAI,CAAC5B,SAAQ,GAAI,IAAI;UAExE,OAAOyB,WAAU,IAAKE,UAAU;QAClC,CAAC,CAAC;MACJ;MACA,OAAOnB,QAAQ;IACjB;EACF,CAAC;EACDqB,OAAO,EAAE;IACPC,gBAAgBA,CAACC,QAAQ,EAAE;MACzB,MAAMZ,QAAO,GAAIY,QAAQ,CAACZ,QAAQ;MAElC,IAAI,IAAI,CAACjB,MAAM,EAAE;QACf,IAAI,CAACR,OAAO,CAACyB,QAAQ,CAAC,CAAC,IAAI,C